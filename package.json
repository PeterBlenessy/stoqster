{
  "name": "stoqster",
  "version": "0.4.8",
  "description": "Follow your favourite stocks and fonds, and your portfolio's performance",
  "productName": "Stoqster",
  "author": "Péter Blénessy",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron",
    "publish:mac": "quasar build -m electron -T mac --publish always",
    "publish:all": "quasar build -m electron -T all --publish always",
    "release:draft": "gh release create $npm_package_version --draft --title $npm_package_version --notes-file CHANGELOG.md",
    "release:publish":  "gh release create $npm_package_version --draft=false --title $npm_package_version --notes-file CHANGELOG.md --repo PeterBlenessy/stoqster-releases",
    "release:mac": "yarn release:draft && yarn publish:mac && yarn release:publish",
    "release:all": "yarn release:draft && yarn publish:all && yarn release:publish"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.6.1",
    "quasar": "^2.0.0",
    "vue-i18n": "^9.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "^3.0.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.4.0",
    "electron-builder-notarize": "^1.2.0",
    "electron-notarize": "^1.1.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.4.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
